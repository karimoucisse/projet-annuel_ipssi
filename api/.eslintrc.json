{"env": {
	"browser": true,
	"commonjs": true,
	"es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
	"ecmaVersion": "latest"
  },
  "rules": {
	"prettier/prettier": "error",
	"import/no-extraneous-dependencies": 0,
	"no-console": 0,
	"no-underscore-dangle": 0
  }}
